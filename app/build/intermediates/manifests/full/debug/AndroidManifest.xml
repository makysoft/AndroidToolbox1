<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tresksoft.toolbox"
    android:installLocation="auto"
    android:versionCode="22"
    android:versionName="2.1.1" >

    <uses-sdk
        android:minSdkVersion="4"
        android:targetSdkVersion="4" />

    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true" />

    <!-- PERMISOS -->
    <uses-permission android:name="android.permission.RESTART_PACKAGES" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <uses-permission android:name="android.permission.GET_PACKAGE_SIZE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.WRITE_APN_SETTINGS" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.CLEAR_APP_CACHE" />

    <application
        android:name="com.tresksoft.toolbox.ProcessApplication"
        android:icon="@drawable/ic_main"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Light" >
        <activity
            android:name="com.tresksoft.toolbox.ActivityProcessManager"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="com.tresksoft.toolbox.VIEW_LIST_PROCESS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tresksoft.toolbox.ActivityProcessManagerIgnore"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="com.tresksoft.toolbox.VIEW_LIST_IGNORE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tresksoft.toolbox.ActivityApplicationManager"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="com.tresksoft.toolbox.VIEW_LIST_APLICACIONES" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tresksoft.toolbox.ActivityPreferences"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.tresksoft.toolbox.ActivityMain"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tresksoft.toolbox.ActivityWifiManager"
            android:label="@string/lbl_activity_wifi"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="com.tresksoft.toolbox.VIEW_LIST_WIFI" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tresksoft.toolbox.ActivityWifiProfile"
            android:label="@string/lbl_activity_wifi"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="com.tresksoft.toolbox.VIEW_LIST_WIFI_PROFILE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tresksoft.toolbox.ActivityBatteryManager"
            android:label="@string/lbl_activity_battery"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="com.tresksoft.toolbox.ACTIVITY_BATTERY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tresksoft.toolbox.ActivityNetworkProfiles"
            android:label="@string/lbl_activity_network_profile"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="com.tresksoft.toolbox.ACTIVITY_NETWORK_PROFILE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tresksoft.toolbox.ActivityNetworkProfilesAdd"
            android:label="@string/lbl_activity_network_profile"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="com.tresksoft.toolbox.ACTIVITY_NETWORK_PROFILE_ADD" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tresksoft.toolbox.ActivityWifiSignalSurvey"
            android:label="@string/lbl_activity_network_profile"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="com.tresksoft.toolbox.ACTIVITY_WIFI_SIGNAL_SURVEY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tresksoft.toolbox.ActivitySpeedSettings"
            android:label="@string/lbl_activity_speed_settings"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="com.tresksoft.toolbox.ACTIVITY_SPEED_SETTINGS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tresksoft.toolbox.ActivityCacheClean"
            android:label="@string/lbl_activity_cache_clean"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="com.tresksoft.toolbox.ACTIVITY_CACHE_CLEAN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tresksoft.toolbox.ActivityMove2SD"
            android:label="@string/lbl_activity_move_sd"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="com.tresksoft.toolbox.ACTIVITY_MOVE_SD" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.google.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize" />

        <!--
     		<service android:name="com.tresksoft.batterymanager.BatteryService"/>
