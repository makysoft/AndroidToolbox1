<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="match_parent">


	<android.support.constraint.ConstraintLayout
		android:orientation="vertical"
		android:layout_width="0dp"
		android:layout_height="453dp"
		android:background="@color/list_item"
		tools:layout_constraintTop_creator="1"
		tools:layout_constraintRight_creator="1"
		app:layout_constraintRight_toRightOf="parent"
		android:id="@+id/nestedScrollView"
		android:layout_marginTop="206dp"
		tools:layout_constraintLeft_creator="1"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintTop_toTopOf="parent">

		<ImageView
			style="@style/Divider"
			android:layout_width="0dp"
			tools:layout_constraintTop_creator="1"
			tools:layout_constraintRight_creator="1"
			android:layout_height="0dp"
			tools:layout_constraintBottom_creator="1"
			app:layout_constraintBottom_toTopOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			android:id="@+id/imageView"
			tools:layout_constraintLeft_creator="1"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintTop_toTopOf="parent" />


		<TextView
			android:id="@+id/textView3"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textColor="@color/white"
			android:textStyle="bold"
			android:text="@string/lbl_cache_cleaner"
			android:textAppearance="?android:attr/textAppearanceMedium"
			tools:layout_constraintTop_creator="1"
			android:layout_marginTop="1dp"
			app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
			tools:layout_constraintLeft_creator="1"
			app:layout_constraintLeft_toLeftOf="@+id/linearLayout2" />


		<TextView
			android:id="@+id/linearLayout2"
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:text="@string/lbl_guiones"
			android:padding="2.0dip"
			tools:layout_constraintRight_creator="1"
			app:layout_constraintRight_toRightOf="parent"
			tools:layout_constraintLeft_creator="1"
			app:layout_constraintLeft_toLeftOf="parent"
			tools:layout_constraintTop_creator="1"
			tools:layout_constraintBottom_creator="1"
			app:layout_constraintBottom_toTopOf="parent"
			app:layout_constraintTop_toTopOf="parent" />

		<TextView
			android:id="@+id/tv_tot_cache"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:gravity="right"
			android:text="Total cache: --"
			android:padding="2.0dip"
			tools:layout_constraintRight_creator="1"
			tools:layout_constraintBottom_creator="1"
			app:layout_constraintBottom_toTopOf="@+id/imageView2"
			app:layout_constraintRight_toRightOf="parent"
			android:layout_marginBottom="5dp"
			android:layout_marginRight="10dp"
			android:layout_marginEnd="10dp" />


		<ImageView
			style="@style/Divider"
			android:layout_height="0dp"
			android:layout_width="0dp"
			app:layout_constraintRight_toLeftOf="@+id/tv_tot_cache"
			tools:layout_constraintTop_creator="1"
			tools:layout_constraintRight_creator="1"
			tools:layout_constraintBottom_creator="1"
			app:layout_constraintBottom_toTopOf="@+id/listView"
			android:id="@+id/imageView2"
			tools:layout_constraintLeft_creator="1"
			app:layout_constraintLeft_toLeftOf="@+id/tv_tot_cache"
			app:layout_constraintTop_toTopOf="@+id/listView" />

		<ListView
			android:id="@+id/listView"
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:divider="@drawable/list_divider"
			android:background="@android:color/transparent"
			tools:layout_constraintTop_creator="1"
			android:layout_marginTop="81dp"
			tools:layout_constraintLeft_creator="1"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			android:layout_marginRight="16dp"
			android:layout_marginEnd="16dp" />

		<LinearLayout
			android:id="@+id/empty"
			android:layout_width="0dp"
			android:layout_height="0dp"
			android:orientation="horizontal"
			android:gravity="center_vertical|center"
			app:layout_constraintRight_toLeftOf="parent"
			tools:layout_constraintTop_creator="1"
			tools:layout_constraintRight_creator="1"
			tools:layout_constraintBottom_creator="1"
			app:layout_constraintBottom_toBottomOf="@+id/imageView3"
			android:layout_marginTop="306dp"
			tools:layout_constraintLeft_creator="1"
			android:layout_marginBottom="306dp"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintTop_toTopOf="@+id/listView">

			<TextView
				android:id="@+id/empty_tv"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="" />
		</LinearLayout>

		<ImageView android:background="@drawable/list_divider_bar"
			android:layout_width="480dp"
			android:layout_height="wrap_content"
			xmlns:android="http://schemas.android.com/apk/res/android"
			android:id="@+id/imageView3"
			tools:layout_editor_absoluteX="0dp"
			app:layout_constraintBottom_toTopOf="@+id/constraintLayout" />

		<android.support.constraint.ConstraintLayout
			android:layout_width="0dp"
			android:layout_height="50dp"
			android:layout_weight="1"
			android:weightSum="1"
			tools:layout_constraintRight_creator="1"
			tools:layout_constraintBottom_creator="1"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintRight_toRightOf="parent"
			tools:layout_constraintLeft_creator="1"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintHorizontal_bias="0.0"
			android:id="@+id/constraintLayout">

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:textColor="@color/bar_icon"
				android:textStyle="bold"
				android:text="@string/lbl_clean"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:id="@+id/textView"
				tools:layout_constraintRight_creator="1"
				app:layout_constraintRight_toRightOf="parent"
				app:layout_constraintLeft_toLeftOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				app:layout_constraintBottom_toBottomOf="parent" />

		</android.support.constraint.ConstraintLayout>

	</android.support.constraint.ConstraintLayout>

	<ImageView
		android:layout_width="0dp"
		android:layout_height="0dp"
		app:srcCompat="@color/ads_background"
		tools:layout_editor_absoluteY="28dp"
		tools:layout_editor_absoluteX="113dp"
		android:id="@+id/imageView4" />


</android.support.constraint.ConstraintLayout>
